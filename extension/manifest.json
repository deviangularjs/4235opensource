{
    "manifest_version": 2,
   "background":{
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "images/logo128.png",
      "default_title": "Search Bookmarked Page Contents"
   },
   "description": "Find your full-text bookmarked and History pages\n  by searching not only their titles and web addresses, but also their  contents!",
   "icons": {
      "128": "images/logo128.png",
      "64":"images/logo64.png",
      "16": "images/logo16.png"
   },
   "minimum_chrome_version": "5.0",
   "name": "CrSearch",
   "permissions": [ "tabs", "bookmarks", "history", "tts","background", "http://*/*", "https://*/*", "unlimited_storage" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "options_page": "options.html",
   "version": "2.5"
}
